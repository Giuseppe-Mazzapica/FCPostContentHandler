{
    "name": "giuseppe-mazzapica/fc-post-content-handler",
    "description": "Post content handler for Fragment Cache",
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Giuseppe-Mazzapica/FCPostContentHandler/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/FCPostContentHandler"
    },
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.4",
        "rarst/fragment-cache": "~1.2"
    },
    "extra": {
        "installer-paths": {
            "vendor/rarst/fragment-cache": ["rarst/fragment-cache"]
        }
    },
    "autoload": {
        "psr-4": {
            "GM\\FCPCH\\": "src/"
        },
        "files": ["fcpch-helpers.php"]
    },
    "config": {
        "optimize-autoloader": true
    }
}
